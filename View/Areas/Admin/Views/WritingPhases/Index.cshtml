@model IEnumerable<Services.DTO.WritingPhases.WritingPhasesDTO>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.WritingPhaseID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AmountArticles)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreateAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModifiedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Is_Opening_registration)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RegistrationPeriodID)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.WritingPhaseID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AmountArticles)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifiedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Is_Opening_registration)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RegistrationPeriodID)
            </td>
            <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.WritingPhaseID }) |
                    @Html.ActionLink("Details", "Details", new { id = item.WritingPhaseID }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.WritingPhaseID })
            </td>
        </tr>
}
    </tbody>
</table>
